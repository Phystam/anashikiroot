#include <TString.h>
#include "TArtEasyScaler.hh"
TArtEasyScaler::TArtEasyScaler(){
  fgated   = 1;
  fungated = 1;
  fBxNEBxH = 1;
  fBxNeuxH = 1;
  fBxCATxH = 1;
  fclock1k = 1;
  fclock1M = 1;

}

TArtEasyScaler::TArtEasyScaler(TString scrfile){
  fgated   = 1;
  fungated = 1;
  fBxNEBxH = 1;
  fBxNeuxH = 1;
  fBxCATxH = 1;
  fclock1k = 1;
  fclock1M = 1;

}
